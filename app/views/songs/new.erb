<h1>New Song Form</h1>

<%= form_for(@song) do |f| %>
  <label>Song Name:</label>
  <%= f.text_field :name %><br>

  <label>Artist Name:</label>
  <%= f.select(:artist_id, @artist_options) %> <br>

  <label>Genre:</label>
  <%= f.select(:genre_id, @genre_options) %><br>

  <%= f.submit %>
<% end %>
